> Name <- c("Jeb","Donald","Ted","Marco","Cody","Hillary","Bernie")
> ABC <- c(4,62,51,21,2,14,15)
> CBS <- c(12,75,43,19,1,21,19)
> poll  <- cbind(Name,ABC,CBS)
> poll
Name      ABC  CBS 
[1,] "Jeb"     "4"  "12"
[2,] "Donald"  "62" "75"
[3,] "Ted"     "51" "43"
[4,] "Marco"   "21" "19"
[5,] "Cody"    "2"  "1" 
[6,] "Hillary" "14" "21"
[7,] "Bernie"  "15" "19"

> poll.df <- data.frame(Name,ABC,CBS)
> poll.df
    Name ABC CBS
1     Jeb   4  12
2  Donald  62  75
3     Ted  51  43
4   Marco  21  19
5    Cody   2   1
6 Hillary  14  21
7  Bernie  15  19


> mean(poll.df[,2])
[1] 24.14286
> mean(poll.df[,3])
[1] 27.14286












































